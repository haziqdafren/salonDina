{
  "name": "salon-muslimah-dina",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "postinstall": "npx prisma generate --schema=./prisma/schema.prisma || true",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset && npm run db:seed",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:setup": "npx prisma generate && npx prisma db push && npm run db:seed",
    "db:migrate": "npx prisma migrate deploy",
    "db:setup-production": "node scripts/setup-production-db.js",
    "db:studio": "npx prisma studio",
    "vercel:build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.12",
    "next": "15.0.3",
    "next-auth": "^4.24.11",
    "pg": "^8.16.3",
    "prisma": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "mini-css-extract-plugin": "^2.9.3",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.20.3",
    "typescript": "^5.6.3"
  }
